{
  "name": "@tasktrove/types",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "exports": {
    "./defaults": {
      "pro": "./src/defaults.pro.ts",
      "default": "./src/defaults.ts"
    },
    "./defaults.base": "./src/defaults.ts",
    "./utils": {
      "pro": "./src/utils.pro.ts",
      "default": "./src/utils.ts"
    },
    "./test-constants": {
      "pro": "./src/test-constants.pro.ts",
      "default": "./src/test-constants.ts"
    },
    "./id": "./src/id.ts",
    "./constants": {
      "pro": "./src/constants.pro.ts",
      "default": "./src/constants.ts"
    },
    "./validators": {
      "pro": "./src/validators.pro.ts",
      "default": "./src/validators.ts"
    },
    "./core": {
      "pro": "./src/core.pro.ts",
      "default": "./src/core.ts"
    },
    "./core.pro": "./src/core.pro.ts",
    "./group": "./src/group.ts",
    "./settings": {
      "pro": "./src/settings.pro.ts",
      "default": "./src/settings.ts"
    },
    "./serialization": {
      "pro": "./src/serialization.pro.ts",
      "default": "./src/serialization.ts"
    },
    "./api-requests": {
      "pro": "./src/api-requests.pro.ts",
      "default": "./src/api-requests.ts"
    },
    "./api-responses": {
      "pro": "./src/api-responses.pro.ts",
      "default": "./src/api-responses.ts"
    },
    "./data-file": {
      "pro": "./src/data-file.pro.ts",
      "default": "./src/data-file.ts"
    },
    "./data-file.base": "./src/data-file.ts",
    "./analytics": "./src/analytics.ts",
    "./team": "./src/team.ts",
    "./notifications": "./src/notifications.ts",
    "./voice-commands": "./src/voice-commands.ts",
    "./api-errors": "./src/api-errors.ts",
    "./reward-levels": {
      "pro": "./src/reward-levels.pro.ts",
      "default": "./src/reward-levels.ts"
    }
  },
  "scripts": {
    "lint": "eslint .",
    "typecheck:base": "tsgo --noEmit",
    "typecheck:pro": "tsgo --project tsconfig.pro.json --noEmit",
    "typecheck": "pnpm typecheck:base && (test -f tsconfig.pro.json && pnpm typecheck:pro || true)",
    "check": "pnpm typecheck && pnpm lint && pnpm test",
    "check:pro": "pnpm typecheck:pro && pnpm lint && pnpm test:pro",
    "test:base": "vitest run --config vitest.config.ts",
    "test:pro": "vitest run --config vitest.config.pro.ts",
    "test": "pnpm test:base && (test -f vitest.config.pro.ts && pnpm test:pro || true)",
    "test:watch": "vitest",
    "test:watch:pro": "vitest --config vitest.config.pro.ts"
  },
  "dependencies": {
    "zod": "catalog:",
    "date-fns": "latest",
    "uuid": "catalog:",
    "@tasktrove/constants": "workspace:*"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@repo/vitest-config": "workspace:*",
    "@typescript/native-preview": "catalog:",
    "@types/uuid": "catalog:",
    "eslint": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:"
  }
}
